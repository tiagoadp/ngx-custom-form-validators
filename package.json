{
  "name": "ngx-custom-form-validators",
  "version": "0.0.1",
  "description": "A class that adds extra form validators",
  "scripts": {
    "start": "tsc && concurrently \"npm run ngc\" \"npm run lite\" ",
    "lite": "lite-server",
    "tsc": "ngc -w",
    "ngc": "ngc",
    "build": "ngc",
    "bundles": "ngc -m system --rootDir ./app --outFile ./bundles/ngx-custom-form-validators.js",
    "build:aot": "rimraf bundles && ngc -p tsconfig-aot.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tiagoadp/ngx-custom-form-validators.git"
  },
  "keywords": [
    "angular2"
  ],
  "author": "Tiago Diaz",
  "license": "MIT",
  "main": "bundles/custom-validator.js",
  "typings": "bundles/custom-validator.d.ts",
  "dependencies": {
    "@angular/common": "^4.0.1",
    "@angular/core": "^4.0.1",
    "@angular/forms": "^4.3.6",
    "@angular/platform-browser": "^4.0.1",
    "@angular/platform-browser-dynamic": "^4.0.1",
    "bootstrap": "4.0.0-alpha.6",
    "core-js": "2.4.1",
    "reflect-metadata": "^0.1.9",
    "rxjs": "^5.3.0",
    "systemjs": "0.19.27",
    "zone.js": "^0.8.5"
  },
  "devDependencies": {
    "@angular/compiler": "^4.0.1",
    "@angular/compiler-cli": "^4.0.1",
    "@angular/platform-server": "^4.0.1",
    "@types/core-js": "^0.9.35",
    "@types/jasmine": "^2.5.40",
    "@types/moment": "^2.13.0",
    "@types/node": "^6.0.54",
    "concurrently": "^3.1.0",
    "lite-server": "^2.2.2",
    "typescript": "^2.5.1"
  }
}
